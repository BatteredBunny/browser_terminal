{
  "manifest_version": 2,
  "name": "Browser terminal",
  "version": "1.3.1",
  "description": "Extension that allows you to open a native shell in the browser",
  "icons": {
    "512": "icons/icon.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "browser_terminal@example.org",
      "strict_min_version": "55.0"
    }
  },
  "permissions": [
    "nativeMessaging"
  ],
  "browser_action": {
    "default_area": "navbar"
  },
  "web_accessible_resources": [
    "html/terminal.html",
    "css/terminal.css",
    "js/terminal.js",
    "icons/icon.png",
    "fonts/Fira Code Regular Nerd Font Complete Mono.ttf",
    "fonts/Fira Code Bold Nerd Font Complete Mono.ttf"
  ],
  "background": {
    "scripts": [
      "/js/background.js"
    ]
  }
}